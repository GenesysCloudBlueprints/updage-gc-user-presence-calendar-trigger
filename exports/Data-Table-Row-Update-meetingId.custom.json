{
  "name": "Data Table Row Update (meetingId)",
  "integrationType": "purecloud-data-actions",
  "actionType": "custom",
  "config": {
    "request": {
      "requestUrlTemplate": "/api/v2/flows/datatables/${input.datatableId}/rows/${input.meetingId}",
      "requestType": "PUT",
      "headers": {
        "Content-Type": "application/json"
      },
      "requestTemplate": "{\n  \"key\": \"${input.meetingId}\",\n  \"TriggerType\": \"${input.TriggerType}\",\n  \"TriggerTime\": \"${input.TriggerTime}\",\n  \"genesysUserId\": \"${input.genesysUserId}\"\n}"
    },
    "response": {
      "translationMap": {},
      "translationMapDefaults": {},
      "successTemplate": "${rawResult}"
    }
  },
  "contract": {
    "input": {
      "inputSchema": {
        "type": "object",
        "required": [
          "datatableId",
          "meetingId"
        ],
        "properties": {
          "datatableId": {
            "type": "string"
          },
          "TriggerType": {
            "type": "string"
          },
          "TriggerTime": {
            "type": "string"
          },
          "genesysUserId": {
            "type": "string"
          },
          "meetingId": {
            "type": "string"
          }
        }
      }
    },
    "output": {
      "successSchema": {
        "type": "object",
        "properties": {}
      }
    }
  },
  "secure": false
}